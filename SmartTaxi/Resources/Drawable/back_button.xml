<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#60ffa800" />
      <stroke
          android:width="1dp"
          android:color="#ffa800" />
      <corners
          android:radius="48dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#ffd800"
          android:endColor="#ffd800"
          android:angle="0" />
      <stroke
          android:width="1dp"
          android:color="#ffa800" />
      <corners
          android:radius="48dp" />
    </shape>
  </item>
</selector>